        android:theme="@style/Theme.WooriOnTact_Parent">
        <activity android:name=".src.mypage.MyPageActivity"></activity>
        <activity android:name=".src.child.CardSettingActivity" />
        <activity android:name=".src.child.ChildInfoActivity" />
        <activity android:name=".src.child.SelectChildActivity" />
        <activity android:name=".src.card.SelectMyCardActivity" />
        <activity android:name=".src.signup.SignUpActivity" />
        <activity android:name=".src.splash.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
    </application>

</manifest>